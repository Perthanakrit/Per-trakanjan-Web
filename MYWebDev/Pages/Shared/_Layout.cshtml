<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MYWebDev</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MYWebDev.styles.css" asp-append-version="true" />
    

</head>
<body data-bs-spy="scroll" data-bs-target=".navbar">
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top">
            <div class="container flex-lg-column">
                <a class="navbar-brand mx-lg-auto mb-lg-4" href="#">
                    <span class="h3 fw-bold d-block d-lg-none" style="color: #f56b4d; font-size: 2.3rem;">Per Trakanjan</span>
                    <img src="./images/IMG_0481.jpg" class="d-none d-lg-block" alt="" />
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="las la-bars"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto flex-lg-column text-lg-center">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#AboutMe">About me</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#portfolio">Achievements</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/Privacy">Contact</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div id="content-wrapper">
        @RenderBody()
        <footer class="py-5 ">
            <div class="container">
                <div class="row gy-4 justify-content-between">
                    <div class="col-auto">
                        <p class="mb-0"> &copy; 2565 - MYWebDev </p>
                    </div>
                    <div class="col-auto">
                        <div class="social-icons">
                            <a href="https://www.instagram.com/p.uler_/?fbclid=IwAR2_bPtSREKAsCz8clU3A5fMsrl6ljrtkH-v7iKwBKx3S7R9ouNJFgiNqnM"><i class="lab la-instagram"></i></a>
                            <a href="https://www.facebook.com/aupper.thanakrit"><i class="lab la-facebook"></i></a>
                            <a href="https://github.com/Perthanakrit/NewRepo"><i class="lab la-github"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12" asp-append-version="true"></script>
    <script>
        var typed = new Typed(".auto-type", {
            strings: ["Student", "Junior Developer"],
            typeSpeed: 150,
            backSpeed: 200,
            loop: true
        })
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>